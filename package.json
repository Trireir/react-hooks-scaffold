{
  "name": "react-hooks-scaffold",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rematch/core": "^1.4.0",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-jss": "^10.4.0",
    "react-redux": "^7.2.2",
    "react-scripts": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
